{
  "apps" : [
    {
      "name" : "app1",
      "script" : "./bin/www",
      "env" : {
        "watch" : true,
        "NODE_ENV" : "development",
        "error_file" : "./log/development_error.log",
        "out_file" : "./log/development_out.log"  
      },
      "env_production" : {
        "NODE_ENV" : "production",
        "exec_mode" : "cluster",
